<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.shortgravity.data-daily</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>/Users/gabriel/Desktop/short_gravity/scripts/data-fetchers/run_all.py</string>
        <string>--cadence</string>
        <string>daily</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>6</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/tmp/sg-data-daily.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/sg-data-daily.log</string>

    <key>RunAtLoad</key>
    <false/>
</dict>
</plist>
